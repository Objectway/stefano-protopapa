{
  "name": "@owui/widget-integration-test",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --mode production --widgetName owui-widget-integration-test",
    "lint": "vue-cli-service lint",
    "build-jar": "node ./scripts/bundle-jar.js --widgetName owui-widget-integration-test",
    "commit": "./node_modules/.bin/git-cz",
    "docker:build-image": "npm run build && IMAGE_VERSION=$(node -p \"require('./package.json').version\") && docker build -t registry.objectway.it/owui/widget-integration-test:latest -t registry.objectway.it/owui/widget-integration-test:$IMAGE_VERSION . && npm run docker:sync-compose-version",
    "docker:push-image": "IMAGE_VERSION=$(node -p \"require('./package.json').version\") && docker push registry.objectway.it/owui/widget-integration-test:latest && docker push registry.objectway.it/owui/widget-integration-test:$IMAGE_VERSION",
    "docker:serve-image": "docker-compose down && docker-compose up -d && docker-compose logs -f",
    "docker:sync-compose-version": "node ../../owui-scripts/widget/update-docker-compose-version.js --widgetName owui-widget-integration-test",
    "lint-fix": "tslint -p tsconfig.json --fix",
    "mock-server": "node mock-server/index.js --port 3010",
    "serve-dist": "npx http-server ./dist",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit"
  },
  "files": [
    "/dist",
    "package.json",
    "package-lock.json",
    "src",
    "README.md",
    "typings.d.ts",
    "bower.json"
  ],
  "dependencies": {
    "@owui-commons/events": "^3.0.0",
    "@owui-commons/http": "^3.0.0",
    "@owui-commons/monitor": "^0.x",
    "@owui-commons/vue-widget-configuration-manager": "^4.0.0",
    "@owui-commons/vue-widget-state-manager": "^1.0.0",
    "@owui-commons/vue-widget-utils": "^1.1.1",
    "@owui/component-spinner": "^2.0.0",
    "@polymer/polymer": "^3.x",
    "@vue/web-component-wrapper": "^1.0.0",
    "inversify": "^5.0.0",
    "lit-element": "^2.1.0",
    "reflect-metadata": "^0.1.0",
    "rxjs": "^6.0.0",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-i18n": "^8.0.0",
    "vue-property-decorator": "^8.1.0",
    "vue-router": "^3.0.0",
    "vuex": "^3.0.0",
    "vuex-class": "^0.3.0",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "7.2.1",
    "@commitlint/config-conventional": "7.1.2",
    "@owui-commons/vue-cli-plugin-owui-preset": "^2.0.0",
    "@owui-commons/webpack": "^0.6.0",
    "@semantic-release/commit-analyzer": "^6.1.0",
    "@types/dotenv": "^6.1.0",
    "@types/jest": "^23.1.4",
    "@vue/cli-plugin-e2e-cypress": "^3.8.0",
    "@vue/cli-plugin-typescript": "^3.8.0",
    "@vue/cli-plugin-unit-jest": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "commitizen": "~2.10.0",
    "cz-conventional-changelog": "~2.1.0",
    "husky": "^1.0.0",
    "lint-staged": "^8.1.5",
    "node-mock-server": "^0.23.0",
    "node-sass": "^4.9.0",
    "read-xml": "^3.0.0",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.0",
    "ts-jest": "~23.10.0",
    "typescript": "^3.4.3",
    "vue-template-compiler": "^2.6.10",
    "webpack-assets-manifest": "^3.0.0",
    "xml2js": "^0.4.0",
    "yargs": "^12.0.0"
  }
}
